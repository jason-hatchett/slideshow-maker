<section class="banner-all">
  <div class="small-12 columns">
    <a href="/">
      <%= image_tag("cloud7.png", :size => "150x150")%>
    </a>
    <a href="/videos" class="button radius" id="searchbutton"> SEARCH VIDEOS </a>
    <a href="/search" class="button radius" id="createbutton"> CREATE VIDEO </a>
  </div>
  <br>
  <br>
</section>


<div class="large-3 large-centered columns">
  <center>
    <%= form_tag("/hashtag/pics") do |f| %>
    <!-- <%= label_tag 'HashTag', nil, :class => "hashtag-font" %> -->
    <br>
    <div class="stay-on-line">

      <i class="fa fa-slack" id='hashtag-symbol'></i>
      <%= text_field_tag :tag, nil, :placeholder => "Enter Your Hashtag" %>
      
    </div>
    <br>
    <%= submit_tag "Search", :class => "button radius", :id => "search-font" %>
    <% end %>
  </center>
</div>

<br>

<div id="instas">
  <%= render "showpics" %>
</div>

